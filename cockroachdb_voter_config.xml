<?xml version="1.0"?>
<parameters>

    <!-- Connection details -->
    <dbtype>cockroachdb</dbtype>
    <driver>org.postgresql.Driver</driver>
    <DBUrl>jdbc:postgresql://127.0.0.1:26257/voter?reWriteBatchedInserts=true</DBUrl>
    <username>root</username>
    <password></password>
    <isolation>TRANSACTION_SERIALIZABLE</isolation>
    
    <scalefactor>10</scalefactor>
    
    <!-- The workload -->
    <terminals>10</terminals>
    <works>
        <work>
          <time>300</time>
          <rate>10000</rate>
          <weights>100</weights>
        </work>
    </works>

    <!-- Voter Procedures declaration -->
    <transactiontypes>
        <transactiontype>
                <name>Vote</name>
        </transactiontype>
    </transactiontypes>
</parameters>
