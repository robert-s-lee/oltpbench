<?xml version="1.0"?>
<parameters>
    <!-- Connection details -->
    <dbtype>cockroachdb</dbtype>
    <driver>org.postgresql.Driver</driver>
    <DBUrl>jdbc:postgresql://127.0.0.1:26257/jpab?reWriteBatchedInserts=true</DBUrl>
    <username>root</username>
    <password></password>
    <isolation>TRANSACTION_SERIALIZABLE</isolation>
    
    <!-- Number of Initial Objects -->
    <scalefactor>1</scalefactor>
    
    <!-- Hibernate configuration -->
    <persistence-unit>Hibernate-MySQL-server</persistence-unit>
    
    <!-- Type of Objects -->
    <testClass>BasicTest</testClass>
      
    <!-- The workload -->  
    <terminals>2</terminals>  
    <works>
        <work>
            <time>10</time>
            <rate>10000</rate>
            <weights>25,25,25,25</weights>
        </work>
    </works>
    
    <!-- JPAB Procedures Declaration -->
    <transactiontypes>
        <transactiontype>
                <name>Persist</name>
        </transactiontype>
        <transactiontype>
                <name>Retrieve</name>
        </transactiontype>
        <transactiontype>
                <name>Update</name>
        </transactiontype>
        <transactiontype>
                <name>Delete</name>
        </transactiontype>
    </transactiontypes>
</parameters>
